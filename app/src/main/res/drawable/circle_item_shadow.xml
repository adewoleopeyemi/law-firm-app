<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    tools:ignore="MissingDefaultResource">

    <!-- Drop Shadow Stack -->
    <item>
        <shape>
            <size
                android:width="1dp"/>
            <solid android:color="@color/t12_colorPrimaryDark" />

            <corners android:radius="@dimen/_50sdp" />
        </shape>
    </item>
    <item>
        <shape>
            <size
                android:width="1dp"/>

            <solid android:color="#05E2E2E2" />

            <corners android:radius="50dp" />
        </shape>
    </item>
    <item>
        <shape>
            <padding
                android:bottom="1.0dp"
                android:left="1.0dp"
                android:right="1.0dp"
                android:top="1.0dp" />

            <solid android:color="#0aE2E2E2" />

            <corners android:radius="@dimen/_35sdp" />
        </shape>
    </item>
    <item>
        <shape>
            <padding
                android:bottom="1.0dp"
                android:left="1.0dp"
                android:right="1.0dp"
                android:top="1.0dp" />

            <solid android:color="#0fE2E2E2" />

            <corners android:radius="@dimen/_50sdp" />
        </shape>
    </item>
    <item>
        <shape>
            <padding
                android:bottom="1.0dp"
                android:left="1.0dp"
                android:right="1.0dp"
                android:top="1.0dp" />

            <solid android:color="#14E2E2E2" />

            <corners android:radius="@dimen/_50sdp" />
        </shape>
    </item>

    <!-- Background -->
    <item
        android:id="@+id/itemShape">
        <shape>
            <solid android:color="@color/white" />

            <corners android:radius="@dimen/_50sdp" />
        </shape>
    </item>

</layer-list>